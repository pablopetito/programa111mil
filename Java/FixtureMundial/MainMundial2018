import java.util.ArrayList;

public class MainMundial2018 {

	public static void main(String[] args) {

		ArrayList <Grupo> torneo = new ArrayList<>();
		
		PantallaTorneo.mostrarEquiposClasificados();
	
		GruposFactory grup = new GruposFactory();
		
		torneo = grup.armarGrupos();
			
		PantallaTorneo.mostrarGrupo(torneo);
	
	}
	
}
